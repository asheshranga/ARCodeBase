	public static map<string,TestKeyValueStore__c> atlasSettings = new map<string,TestKeyValueStore__c>();

	//------------------------------------------------------------------------------------
	// Get ATLAS getValueTestKeyValueStoreForAtlas
	//------------------------------------------------------------------------------------
	public static string getValue(string code) 
	{
		if (atlasSettings == null || atlasSettings.size()==0)
			atlasSettings = getProjectSettings('Atlas');
		return atlasSettings.get(code).Value__c;
	}

	//------------------------------------------------------------------------------------
	// Get ATLAS getValueTestKeyValueStoreForAtlas
	//------------------------------------------------------------------------------------
	public static String getMetadata(string code) 
	{
		if (atlasSettings == null || atlasSettings.size()==0)
			atlasSettings = getProjectSettings('Atlas');
		return atlasSettings.get(code).Metadata__c;
	}
	
	//------------------------------------------------------------------------------------
	// Get ALL settings (Value & metadata) for the Project
	//------------------------------------------------------------------------------------
	private static map<string,TestKeyValueStore__c> getProjectSettings(string project) 
	{
		list<TestKeyValueStore__c> objs = [SELECT code__c, Value__c, Metadata__c FROM TestKeyValueStore__c WHERE Project__c =: project];
		
		map<string,TestKeyValueStore__c> testSettings = new map<string,TestKeyValueStore__c>();
		for (TestKeyValueStore__c o : objs)
		{
			testSettings.put(o.code__c, o);
		}
		
		return testSettings;
	}	
   
